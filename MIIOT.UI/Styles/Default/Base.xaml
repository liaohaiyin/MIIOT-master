    <SolidColorBrush x:Key="Nb.AppendBtn.PressedBorderBrush" Color="#999999" Opacity="0.5"></SolidColorBrush>
    <SolidColorBrush x:Key="Nb.AppendBtn.Background" Color="#FFFFFF"></SolidColorBrush>
